{
  "name": "Start",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Demo of language app",
  "icons": {
	"16" : "imgs/icon.png",
    "48" : "imgs/icon48.png",
    "128" : "imgs/icon128.png"
  },
  "options_page": "options.html",
  "browser_action": {
      "default_title": "",
      "default_icon": "imgs/icon.png",
      "default_popup": "popup.html"
  }, 
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["http://*/*","https://*/*"],
      "js" : ["contentscript.js"]
    }
  ]
}
